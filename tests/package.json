{
  "name": "lyvo-selenium-tests",
  "version": "1.0.0",
  "description": "Selenium tests for Lyvo login functionality",
  "main": "tests/selenium/login.test.js",
  "scripts": {
    "test": "node tests/selenium/login.test.js",
    "test:headless": "HEADLESS=true node tests/selenium/login.test.js",
    "test:debug": "HEADLESS=false SLOW_MO=2000 node tests/selenium/login.test.js",
    "install-deps": "npm install selenium-webdriver chai mongoose bcryptjs dotenv",
    "create-test-users": "node create_test_users.js",
    "setup": "npm run install-deps && npm run create-test-users"
  },
  "dependencies": {
    "selenium-webdriver": "^4.15.0",
    "chai": "^4.3.10",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/selenium-webdriver": "^4.1.20",
    "@types/chai": "^4.3.8"
  },
  "keywords": [
    "selenium",
    "testing",
    "login",
    "lyvo",
    "automation"
  ],
  "author": "Lyvo Team",
  "license": "MIT"
}
